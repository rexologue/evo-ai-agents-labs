{
  "rawEnvs": {
    "PORT": {
      "isRequired": false,
      "description": "Порт MCP сервера",
      "defaultValue": "8000"
    },
    "HOST": {
      "isRequired": false,
      "description": "Хост MCP сервера",
      "defaultValue": "0.0.0.0"
    },
    "DB_HOST": {
      "isRequired": true,
      "description": "Хост PostgreSQL"
    },
    "DB_PORT": {
      "isRequired": true,
      "description": "Порт PostgreSQL",
      "defaultValue": "5432"
    },
    "DB_NAME": {
      "isRequired": true,
      "description": "Имя базы данных"
    },
    "DB_USER": {
      "isRequired": true,
      "description": "Пользователь базы данных",
      "defaultValue": "postgres"
    }
  },
  "secretEnvs": {
    "DB_PASSWORD": {
      "isRequired": true,
      "description": "Пароль пользователя PostgreSQL"
    }
  }
}
